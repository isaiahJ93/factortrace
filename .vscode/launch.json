{
  "name": "Run FactorTrace CLI Demo",
  "type": "python",
  "request": "launch",
  "module": "factortrace",
  "justMyCode": false,
  "console": "integratedTerminal",
  "env": {
  "PYTHONPATH": "${workspaceFolder}/src"
}
}
{
  "name": "Run FastAPI App",
  "type": "python",
  "request": "launch",
  "module": "uvicorn",
  "args": [
    "src.api.main:app",
    "--reload"
  ],
  "console": "integratedTerminal"
}

{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Voucher Generator",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/voucher_generator.py",
      "console": "integratedTerminal"
    }
  ]
}

{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run XHTML Generator",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/src/main.py",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}